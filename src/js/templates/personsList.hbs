<div class="b-list">
	<ul>
		{{#each persons}}
			<li {{#if this.active}}class="active"{{/if}} data-id={{this.id}}>
				<a href="#">
					<span>{{this.first_name}}</span>
					<span>{{this.age}}</span>
				</a>
			</li>
	  	{{/each}}
	</ul>
</div>
<div class="b-navigate">
	<a class="b-navigate__prev" href="#"><---</a>
	<span>{{currendId}}</span>
	<a class="b-navigate__next"  href="#">---></a>
</div>