<div class="b-list">
	<ul>
		{{#each persons}}
			<li {{#if this.active}}class="active"{{/if}}>
				<a href="/#/person/{{this.id}}">
					<span>{{this.first_name}}</span>
					<span>{{this.age}}</span>
				</a>
			</li>
	  	{{/each}}
	</ul>
</div>
<div class="b-navigate">
	<a href="#/prev"><---</a>
	<span>{{currendId}}</span>
	<a href="#/next">---></a>
</div>